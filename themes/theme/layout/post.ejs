<div class="row">
  <article class="col-md-9 article">
    <h1 class="article__title">
      <%- page.title %>
    </h1>

    <h1>
      Post by
      <a href="https://www.facebook.com/Love.Ren.348" target="_blank"><%- page.author %></a>
      on <%- page.date.format('D-MM-YYYY') %>
    </h1>

    <div>
      Tags: <% page.tags.map(function(tag){ %>
      <a href="<%- config.root %><%- tag.path %>">
        <%= tag.name %>
      </a>
      <% }) %>
    </div>

    <div>
      Content: <%- page.content %>
    </div>
  </article>
  <aside class="col-md-3 toc">
    <strong class="toc__title">CONTENTS</strong>
    <% if(toc(page.content)){ %> <%-
    toc(page.content,{list_number:true}) %> <% } else { %>
    <% } %>
  </aside>
</div>